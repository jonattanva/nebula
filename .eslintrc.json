{
  "extends": [
    "eslint:recommended",
    "plugin:jsdoc/recommended",
    "plugin:prettier/recommended",
    "prettier"
  ],
  "plugins": ["jsdoc", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "jsdoc/require-returns-description": "off"
  }
}
